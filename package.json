{
  "name": "foundryvtt-module-template",
  "version": "0.0.4",
  "description": "",
  "main": "dist/module.js",
  "scripts": {
    "start": "docker compose up --build",
    "stop": "docker compose down",
    "watch": "npx onchange src/**/* -- yarn build",
    "build": "node build.mjs",
    "build:prod": "NODE_ENV=production node build.mjs",
    "test:pre": "yarn test:clean",
    "test:mid": "mv cypress/reports/videos cypress/reports/videos-temp",
    "test:post": "mv cypress/reports/videos-temp/* cypress/reports/videos && npx rimraf cypress/reports/videos-temp",
    "test:component": "npx cypress run --component --browser chrome && mv cypress/reports/index.html cypress/reports/component.html",
    "test:e2e": "npx start-server-and-test start http://localhost:30000 'npx cypress run --e2e --browser chrome && mv cypress/reports/index.html cypress/reports/e2e.html' || docker compose down",
    "test": "yarn test:pre && yarn test:component && yarn test:mid && yarn test:e2e && yarn test:post",
    "test:clean": "npx rimraf cypress/reports"
  },
  "author": "Unarekin",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.25.7",
    "@babel/preset-env": "^7.25.7",
    "@cypress/webpack-preprocessor": "^6.0.2",
    "@eslint/js": "^9.11.0",
    "@types/Foundry-VTT": "https://github.com/League-of-Foundry-Developers/foundry-vtt-types.git",
    "@types/cypress": "^1.1.3",
    "@types/eslint__js": "^8.42.3",
    "@types/handlebars": "^4.1.0",
    "@types/node": "^22.7.5",
    "@types/path-browserify": "^1.0.3",
    "@types/unzipper": "^0.10.10",
    "babel-loader": "^9.2.1",
    "cypress-ct-html": "^1.1.0",
    "cypress-mochawesome-reporter": "^3.8.2",
    "cypress-multi-reporters": "^1.6.4",
    "deepmerge-ts": "^7.1.0",
    "docker-compose": "^1.1.0",
    "dotenv": "^16.4.5",
    "esbuild": "^0.23.1",
    "esbuild-copy-static-files": "^0.1.0",
    "esbuild-node-externals": "^1.14.0",
    "esbuild-plugin-clean": "^1.0.1",
    "esbuild-plugin-json-merge": "^0.1.0",
    "esbuild-sass-plugin": "^3.3.1",
    "eslint": "^9.11.0",
    "glob": "^11.0.0",
    "handlebars": "^4.7.8",
    "marge": "^1.0.1",
    "mocha": "^10.7.3",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "path-browserify": "^1.0.1",
    "ts-loader": "^9.5.1",
    "typedoc": "^0.26.8",
    "typedoc-github-wiki-theme": "^2.0.0",
    "typedoc-plugin-markdown": "^4.2.9",
    "typedoc-umlclass": "^0.10.0",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.6.0",
    "unzipper": "^0.12.3",
    "vite": "^5.4.9",
    "webpack": "^5.95.0",
    "yocto-spinner": "^0.1.0"
  }
}
